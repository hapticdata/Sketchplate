{
	"editor": "Sublime Text 2 (osx)",
	"editors": {
		"BBEdit (osx)": [
			"bbedit",
			"%path"
		],
        "Sublime Text 3 (osx)": [
			"/Applications/Sublime Text.app/Contents/SharedSupport/bin/subl",
			"%path"
        ],
		"Sublime Text 2 (osx)": [
			"/Applications/Sublime Text 2.app/Contents/SharedSupport/bin/subl",
			"%path"
		],
		"Sublime Text - in $PATH (run `subl` process)": [
			"subl",
			"%path"
		],
		"Textmate (osx)": [
			"mate",
			"%path"
		],
		"Vim": [
			"vim",
			"%path"
		],
		"Vim - in new terminal (osx)": [
			"osascript",
			"-e",
			"tell application \"Terminal\" to do script \"vim %path\""
		],
		"WebStorm": [
			"wstorm",
			"%path"
		]
	},
	"template": "amd-sketch",
	"templatesPath": "~/.sketchplate/templates"
}
